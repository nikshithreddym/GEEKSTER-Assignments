import java.util.Scanner;
public class count_digits
{
    public static void main(String[] args) 
    {
        int num;
        int count = 0;
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the number:");
        num = s.nextInt()
       
        while (num != 0) {
            num = num / 10;
            ++count;
        }
        return count;
    }
}
