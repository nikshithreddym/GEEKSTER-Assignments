import java.util.Scanner;
public class Perfect_square
{
    public static void main(String[] args) 
    {
        int num;
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the number:");
        num = s.nextInt()
        
        for(i=0;i<=num/2;i++){
          if ((num % i == 0) && (num / i == i)) {
                return true;
            }
        }
        return false;
    }
}
